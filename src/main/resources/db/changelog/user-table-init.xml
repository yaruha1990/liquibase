        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
	  http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="2" author="yarik">
        <preConditions onFail="CONTINUE">
            <sqlCheck expectedResult="0">select count(*) from user</sqlCheck>
        </preConditions>

        <insert tableName="user">
            <column name="email" value="yaruha@ukr.net"></column>
            <column name="password" value="123"></column>
            <column name="name" value="yaruha"></column>
        </insert>
    </changeSet>

    <changeSet id="3" author="yarik" runOnChange="true">
        <insert tableName="user">
            <column name="email" value="yana@ukr.net"></column>
            <column name="password" value="321"></column>
            <column name="name" value="yana"></column>
        </insert>
    </changeSet>

</databaseChangeLog>